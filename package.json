{
  "name": "vue-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "bun run vite",
    "build": "bun run run-p type-check \"build-only {@}\" --",
    "preview": "bun run vite preview",
    "test:unit": "bun run vitest",
    "build-only": "bun run vite build",
    "type-check": "bun run vue-tsc --build",
    "lint": "bun run eslint . --ext .js,.jsx,.ts,.tsx,.vue --fix --ignore-path .gitignore",
    "format": "bun run prettier --write \"**/*.{js,jsx,ts,tsx,json,md,vue}\"",
    "lint:fix": "bun run lint --fix",
    "prepare": "bun run husky install",
    "postinstall": "bun run prepare",
    "reinstall": "rm -rf node_modules && bun install"
  },
  "dependencies": {
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.16.5",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-vue": "^10.3.0",
    "eslint-plugin-vue-pug": "0.6.2",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "pug": "3.0.3",
    "typescript": "~5.8.0",
    "unplugin-vue-components": "28.8.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.4"
  }
}
